# Moneybird Configuration
# Option 1: OAuth (for REST API fallback)
MONEYBIRD_CLIENT_ID=
MONEYBIRD_CLIENT_SECRET=
MONEYBIRD_ACCESS_TOKEN=
MONEYBIRD_ADMINISTRATION_ID=

# Option 2: Token (for MCP or direct API)
MONEYBIRD_TOKEN=

# MCP Server Configuration
MCP_TRANSPORT=http
MCP_SERVER_URL=https://moneybird.com/mcp/v1/read_write
MCP_SERVER_AUTH_TOKEN=

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o

# Optional: Anthropic Claude
ANTHROPIC_API_KEY=

# Database
DATABASE_PATH=./data/moneybird-agent.db

# Confidence Thresholds (0-100)
CONFIDENCE_AUTO_THRESHOLD=95
CONFIDENCE_REVIEW_THRESHOLD=80

# Amount Threshold (in cents)
AMOUNT_REVIEW_THRESHOLD=100000

# Scheduler
CRON_SCHEDULE=0 * * * *

# Logging
LOG_LEVEL=info

# Email Notifications (Optional)
EMAIL_ENABLED=false
EMAIL_SMTP_HOST=
EMAIL_SMTP_PORT=587
EMAIL_SMTP_SECURE=false
EMAIL_SMTP_USER=
EMAIL_SMTP_PASS=
EMAIL_FROM=
EMAIL_TO=

# WhatsApp Notifications (Optional)
WHATSAPP_ENABLED=false
WHATSAPP_PROVIDER=twilio
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_WHATSAPP_FROM=
WHATSAPP_TO=

# Notification Settings
NOTIFICATIONS_ENABLED=true
NOTIFICATION_ERRORS_ONLY=false
