# Moneybird MCP (Required)
MCP_SERVER_URL=https://moneybird.com/mcp/v1/read_write
MCP_SERVER_AUTH_TOKEN=your_bearer_token
MONEYBIRD_ADMINISTRATION_ID=your_administration_id

# OpenAI (Required)
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o

# Database (Optional - defaults shown)
DATABASE_PATH=./data/moneybird-agent.db

# Confidence Thresholds (Optional - defaults shown)
CONFIDENCE_AUTO_THRESHOLD=95
CONFIDENCE_REVIEW_THRESHOLD=80
AMOUNT_REVIEW_THRESHOLD=100000

# Scheduler (Optional - defaults shown)
CRON_SCHEDULE=0 * * * *
LOG_LEVEL=info

# Email Notifications (Optional - only set if you want email)
EMAIL_SMTP_HOST=smtp.example.com
EMAIL_SMTP_PORT=587
EMAIL_SMTP_USER=your_email@example.com
EMAIL_SMTP_PASS=your_password
EMAIL_TO=recipient@example.com

# WhatsApp Notifications (Optional - only set if you want WhatsApp)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_WHATSAPP_FROM=whatsapp:+14155238886
WHATSAPP_TO=+1234567890

# Moneybird OAuth (Optional - only for REST API fallback)
MONEYBIRD_CLIENT_ID=your_client_id
MONEYBIRD_CLIENT_SECRET=your_client_secret
MONEYBIRD_ACCESS_TOKEN=your_access_token
